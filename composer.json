        "illuminate/database": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*",
        "illuminate/http": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
